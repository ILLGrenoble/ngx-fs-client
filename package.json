{
    "name": "@illgrenoble/ngx-fs-client",
    "description": "Ngx-fs-client is an angular component providing access to a remote filesystem via the Node FS Server, performing standard file system operations similar to a standard file manager.",
    "version": "0.9.4",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --proxy-config proxy.conf.json",
        "build:lib": "ng build lib --configuration production",
        "postbuild:lib": "cp ./README.md ./dist/lib && cp ./LICENSE ./dist/lib",
        "watch": "ng build --watch --configuration development",
        "publish:lib": "npm run build:lib && npm publish ./dist/lib --access public"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ILLGrenoble/ngx-fs-client.git"
    },
    "homepage": "https://github.com/ILLGrenoble/ngx-fs-client",
    "license": "GPL-3.0",
    "author": "Stuart Caunt <caunt@ill.fr>",
    "files": [
        "dist",
        "LICENSE",
        "README.md"
    ],
    "keywords": [
        "angularjs",
        "angular",
        "angular2",
        "javascript",
        "files",
        "fs",
        "filesystem"
    ],
    "dependencies": {
        "@angular/animations": "^16.0.0",
        "@angular/cdk": "~16.2.6",
        "@angular/common": "^16.0.0",
        "@angular/compiler": "^16.0.0",
        "@angular/core": "^16.0.0",
        "@angular/forms": "^16.0.0",
        "@angular/material": "~16.2.6",
        "@angular/platform-browser": "^16.0.0",
        "@angular/platform-browser-dynamic": "^16.0.0",
        "@angular/router": "^16.0.0",
        "ngx-drag-drop": "^16.1.0",
        "rxjs": "~7.8.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.13.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^16.0.6",
        "@angular/cli": "~16.0.3",
        "@angular/compiler-cli": "^16.0.0",
        "ng-packagr": "^16.0.0",
        "typescript": "~5.0.2"
    }
}
